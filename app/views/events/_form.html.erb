<%= error_messages_for(f.object) %>
<table summary="event form fields">
  <tr>
    <th>Name</th>
    <td><%= f.text_field(:name) %></td>
  </tr>
  <tr>
    <th>Location</th>
    <td><%= f.text_field(:location) %></td>
  </tr>
  <tr>
    <th>Start</th>
    <td><%= f.datetime_field(:start_time) %></td>
  </tr>
  <tr>
    <th>End</th>
    <td><%= f.datetime_field(:end_time) %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= f.text_field(:description) %></td>
  </tr>
</table>
